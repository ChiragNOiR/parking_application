generator client {
    provider = "prisma-client-js"
}

datasource db {
    url      = env("DATABASE_URL")
    provider = "mongodb"
}

model User {
    id         String   @id @default(auto()) @map("_id") @db.ObjectId
    email      String   @unique
    fname      String
    username   String
    passweord  String
    created_at DateTime @default(now())
    updated_at DateTime @updatedAt
}
